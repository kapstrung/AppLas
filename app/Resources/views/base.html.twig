<!-- app/Resources/views/base.html.twig -->
<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}AppLas{% endblock %} - AppLas</title>  
        {% block stylesheets %}
            {% stylesheets 
                '@AppLasSiteBundle/Resources/public/bootstrap3/css/bootstrap.min.css'
                '@AppLasSiteBundle/Resources/public/css/styl.less'
            %}
            <link rel="stylesheet" href="{{ asset_url }}">     
            {% endstylesheets %}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />    
    </head>
    <body onload="mapaStart()">
        <div id="mapka" style="border: 1px solid black; background: gray;">  
        <!-- tu bÄ™dzie mapa -->  
        </div> 
        {% block body %}
        {% endblock %}
        {% block javascripts %}
            {% javascripts
                '@AppLasSiteBundle/Resources/public/js/jquery.js'
                '@AppLasSiteBundle/Resources/public/js/mapStart.js'
            %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %} 
                <script src="http://maps.google.com/maps/api/js?sensor=false" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>